{
  "name": "customer-service",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "start": "NODE_PATH=src ts-node src/app.ts",
    "start:dev": "NODE_PATH=src nodemon",
    "db:create-migration": "knex --knexfile src/infrastructure/db/knexfile.ts migrate:make",
    "db:migrate": "knex --knexfile src/infrastructure/db/knexfile.ts migrate:latest",
    "db:migrate-undo": "knex --knexfile src/infrastructure/db/knexfile.ts migrate:rollback",
    "db:load": "knex seed:run --knexfile src/infrastructure/db/knexfile.ts",
    "db:create-seed": "knex --knexfile src/infrastructure/db/knexfile.ts seed:make",
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/faker": "^4.1.4",
    "@types/humps": "^1.1.2",
    "@types/jest": "^23.3.10",
    "@types/knex": "^0.15.1",
    "@types/koa": "^2.0.47",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-router": "^7.0.35",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.12",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "http-shutdown": "^1.2.0",
    "humps": "^2.0.1",
    "io-ts": "^1.4.2",
    "io-ts-reporters": "^0.0.21",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.3.0",
    "kcors": "2.2.2",
    "knex": "^0.13.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^3.5.1",
    "koa-logger": "^3.2.0",
    "koa-response-time": "^2.0.0",
